<!doctype html>
<html class="no-js" lang="{{ shop.locale }}">
  <head> 
        
    {% if template contains 'collection' %}
    <style>
      .template-collection { 
        overflow: auto !important;
      }
    </style>
    <style>
      a.btn.klaviyo-bis-trigger {
        width: 100% !important;
      }
    </style>
    {% endif %}
    {% include 'social-meta-tags' %} 
    {% include 'css-variables' %} 
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.color_button }}">
    <link rel="canonical" href="{{ canonical_url }}">
    {% if settings.favicon != blank %}<link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">{% endif %}  
    <small class="visually-hidden" style="color:{{ settings.color_body_bg }};background:{{ settings.color_body_bg }}"> {{settings.lk_store}} </small>     
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    <link rel="preload" href="https://cdn.shopify.com/s/files/1/0635/1231/4085/t/42/assets/fontawesome-webfont.woff2?v=32207045845775034261661418784" as="font" type="font/woff2" crossorigin>
